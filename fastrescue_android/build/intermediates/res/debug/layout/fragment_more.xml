<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/action_layout"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/color_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/label_main_menu4"
            android:textColor="@color/color_black"
            android:textSize="@dimen/large_text_size" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@color/color_line" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/action_layout"
        android:background="@color/color_gray3"
        android:orientation="vertical"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <RelativeLayout
            android:id="@+id/notice"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_more_public"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon14" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />

        <RelativeLayout
            android:id="@+id/aboutE"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_more_aboutEhelp"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon14" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />
        <RelativeLayout
            android:id="@+id/version"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_more_updateVersion"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />

            <TextView
                android:id="@+id/more_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:layout_toLeftOf="@+id/arrow" />

            <ImageView
                android:id="@+id/arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon14" />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_more_aboutus"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />


            <Button
                android:id="@+id/callUs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@color/color_gray3"
                android:text="@string/label_telephone_number"
                android:textColor="@color/color_orange" />


        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />
        <RelativeLayout
            android:id="@+id/handbook"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_handbook"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon14" />


        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />
        <RelativeLayout
            android:id="@+id/QR_code"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/label_QR_code"
                android:textColor="@color/color_black2"
                android:textSize="@dimen/medium_text_size2" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon14" />


        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/color_line" />


        <Button
            android:id="@+id/logout"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@color/color_gray3"
            android:gravity="center"
            android:text="@string/label_logout"
            android:textColor="@color/color_black" />
        <Button
            android:id="@+id/onlytest"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@color/color_gray3"
            android:onClick="test"
            android:text="我只是看看的！"
            android:textColor="@color/color_black" />



    </LinearLayout>

    <include layout="@layout/progressbar_layout" />
    <RelativeLayout
        android:id="@+id/cherk"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:visibility="gone"
        android:layout_height="wrap_content">
        <include
            android:id="@+id/cherkVersion_bar"
            layout="@layout/progressbar_layout"
            android:visibility="visible" />
        <TextView
            android:textSize="14sp"
            android:layout_below="@+id/cherkVersion_bar"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/label_more_cherkVersion"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ProgressBar
            android:progressDrawable="@drawable/progressbar"
            android:visibility="invisible"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="30dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/progressBar_download"
            android:max="100"
            android:progress="0"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:visibility="gone"
            android:layout_marginTop="40dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text=" "
            android:textSize="20sp"
            android:textColor="@color/color_green"
            android:id="@+id/progress_text"
            android:layout_below="@+id/progressBar_download"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>

</RelativeLayout>
