<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@+id/back"
    android:keepScreenOn="true"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_marginTop="22.5dp"
        android:id="@+id/title_layout"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/color_white"
        android:visibility="visible">
        <RelativeLayout
            android:id="@+id/aboutE_return"
            android:layout_width="50dp"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/image"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="11dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/btn_back_black"
                />
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44.5dp"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="@string/label_main_menu7"
            android:textColor="@color/color_black"
            android:textSize="@dimen/large_text_size" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@color/color_line" />

    </RelativeLayout>
    <LinearLayout
        android:layout_alignTop="@+id/title_layout"
        android:id="@+id/log"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:text=""
            android:textSize="12sp"
            android:textColor="@color/color_gray1"
            android:id="@+id/aboutE_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <ImageView
        android:layout_marginTop="40dp"
        android:src="@drawable/qr_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_below="@+id/title_layout"
        android:layout_centerHorizontal="true" />
    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_ehelp"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />
    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_ehelp_QR"
        android:textColor="@color/color_gray1"
        android:layout_below="@+id/text1"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />
    <TextView
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_QR_QR"
        android:id="@+id/textView3"
        android:layout_below="@+id/text2"
        android:layout_centerHorizontal="true" />
    <LinearLayout
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout2">

        <TextView
            android:gravity="center_horizontal"
            android:textColor="@color/color_gray1"
            android:text="@string/text_ehelp_owner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:gravity="center_horizontal"
            android:textColor="@color/color_gray1"
            android:text="Copyright @ 2015 Auto-SOS.CN All Rights Reserved "
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>

</RelativeLayout>
